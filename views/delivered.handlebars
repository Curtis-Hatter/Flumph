{{! delivered orders begins }}
<div class="row">
    <div class="container col-12 text-center header-style my-3">
        <img src="/img/delivered-orders.svg" width="75%">
    </div>
</div>

<div class="row">
    <div class="col-1">

    </div>
    <div class="container col-10 shadow-lg my-3 rounded white2background">
        <div class="row py-4 px-5 border-bottom border-secondary">
            <div class="col-10">
                <div class="row">
                    <h1><b>Name of Package</b></h1>
                </div>
                <div class="row">
                    <a class = "link-color" href="https//:www.google.com/search?9400109206094987262729" alt="Tracking Number">
                        <h2>Tracking Number</h2>
                    </a>
                </div>
            </div>
            <h3 class="col-2"><img src="/img/fedex.svg" width="100%"></h3>
        </div>
        <div class="row p-5">
            <h2><i>Item Description</i></h2>
        </div>
        <div>
            <img src="/img/delivered.svg" class="mb-3" alt="Your order has been delivered">
        </div>
    </div>
    <div class="col-1">
    </div>
</div>


{{#each shipments}}
{{#if delivered}}
    <div class="row">
        <div class="col-1">
            {{!-- empty column --}}
        </div>
        <div class="container col-10 shadow-lg my-3 rounded white2background">
            <div class="row py-4 px-5 border-bottom border-secondary">
                <div class="col-10">
                    <div class="row">
                        <h1><b>{{this.title}}</b></h1>
                    </div>
                    <div class="row">
                        <a class = "link-color" href="https://www.google.com/search?q=9400111202555339949769" alt="Tracking Number">
                            <h2>{{this.tracking}}</h2>
                        </a>
                    </div>
                </div>
                <h3 class="col-2"><img src={{this.carrier}} width="100%"></h3>
            </div>
            <div class="row p-5">
                <h2><i>{{this.description}}</i></h2>
            </div>
            <div>
                <img src="/img/delivered.svg" class="mb-3" alt="Your order has been placed">
            </div>
        </div>
        <div class="col-1">
            {{!-- empty column --}}
        </div>
    </div>
{{/if}}
{{/each}}
</div>

